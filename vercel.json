{
    "version": 2,
    "builds": [
        {
            "src": "frontend/main.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/public/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/frontend/main.js"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/public/$1"
        }
    ]
}